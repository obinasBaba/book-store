{
  "name": "tv-guide",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <hey@joaopedro.dev> (@jpedroschmitz)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "codegen": "graphql-codegen --config src/lib/codgen/client-config.ts",
    "codegen:watch": "graphql-codegen --watch --config src/lib/codgen/client-config.ts",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.7.17",
    "@emotion/react": "^11.10.8",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.8",
    "@fullcalendar/core": "6.1.8",
    "@fullcalendar/daygrid": "6.1.8",
    "@fullcalendar/interaction": "6.1.8",
    "@fullcalendar/list": "6.1.8",
    "@fullcalendar/multimonth": "6.1.8",
    "@fullcalendar/react": "6.1.8",
    "@fullcalendar/resource-timeline": "6.1.8",
    "@fullcalendar/timegrid": "6.1.8",
    "@graphql-yoga/apollo-link": "2.0.3",
    "@mui/icons-material": "5.14.1",
    "@mui/lab": "5.0.0-alpha.137",
    "@mui/material": "5.14.2",
    "@tanstack/react-query": "^4.32.1",
    "@tanstack/react-query-devtools": "^4.32.1",
    "axios": "1.4.0",
    "clsx": "1.2.1",
    "color2k": "^2.0.2",
    "formik": "2.4.2",
    "framer-motion": "^10.12.5",
    "graphql": "16.7.1",
    "gsap": "3.12.1",
    "lodash.debounce": "^4.0.8",
    "next": "^13.3.4",
    "next-auth": "4.22.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.1",
    "react-simple-pull-to-refresh": "1.3.3",
    "recoil": "0.7.7",
    "swiper": "10.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash.debounce": "4.0.7",
    "@types/node": "18.11.9",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "sass": "1.56.1",
    "typescript": "5.0.4"
  }
}
